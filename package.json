{
  "name": "val-design",
  "version": "0.0.46",
  "private": false,
  "license": "MIT",
  "description": "React UI component library",
  "author": "Cupid Valentine",
  "keywords": [
    "val-design",
    "front-end",
    "CSS",
    "Sass",
    "React",
    "Component",
    "UI",
    "TypeScript",
    "React Hooks"
  ],
  "homepage": "https://valcosmos.github.io/val-design",
  "repository": {
    "type": "git",
    "url": "https://github.com/valcosmos/val-design.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint --fix --ext js,ts,tsx src --max-warnings 5",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "sass --embed-sources ./src/styles/index.scss ./dist/index.css && sass --embed-sources --style compressed ./src/styles/index.scss ./dist/index.min.css",
    "clean": "rimraf ./dist",
    "build": "yarn clean && yarn lint && yarn build-es && yarn build-umd",
    "prepublishOnly": "yarn build",
    "prepare": "husky install",
    "test": "vitest",
    "build-es": "rollup --config rollup.esm.config.js --bundleConfigAsCjs",
    "build-umd": "rollup --config rollup.umd.config.js --bundleConfigAsCjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "async-validator": "^4.2.5",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-json": "^5.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^13.4.0",
    "@types/lodash-es": "^4.17.6",
    "@types/react-dom": "^18.0.6",
    "@types/react-transition-group": "^4.4.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.25.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.6",
    "happy-dom": "^7.5.12",
    "husky": "^8.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.22",
    "rollup-plugin-sass": "^1.12.16",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.55.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  }
}
