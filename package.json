  "files": [
    "dist"
  ],
  "main": "dist/main.js",
  "module": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint --fix --ext js,ts,tsx src --max-warnings 5",
